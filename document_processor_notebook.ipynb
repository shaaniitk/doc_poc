    "## 4. Document Processing"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Initialize processor with current configuration\n",
    "def create_processor():\n",
    "    # Update config with widget values\n",
    "    current_config = {\n",
    "        'llm': {\n",
    "            'provider': llm_provider.value,\n",
    "            'model': model_name.value,\n",
    "            'max_tokens': max_tokens.value,\n",
    "            'temperature': temperature.value,\n",
    "            'output_format': output_format.value\n",
    "        },\n",
    "        'processing': {\n",
    "            'template': template_type.value,\n",
    "            'enable_enhancement': enable_enhancement.value,\n",
    "            'chunk_strategy': 'semantic'\n",
    "        },\n",
    "        'output': {\n",
    "            'directory': 'outputs',\n",
    "            'format': output_format.value,\n",
    "            'generate_analysis': True\n",
    "        }\n",
    "    }\n",
    "    \n",
    "    return DocumentProcessor('config.yaml'), current_config\n",
    "\n",
    "# Get document source based on input type\n",
    "def get_document_source():\n",
    "    if input_type.value == 'Local File':\n",
    "        return file_path.value\n",
    "    elif input_type.value == 'URL':\n",
    "        return url_input.value\n",
    "    else:  # arXiv ID\n",
    "        return f\"arxiv:{arxiv_id.value}\"\n",
    "\n",
    "print(\"üöÄ Ready to process document\")\n",
    "print(f\"Current source: {get_document_source()}\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Process document button\n",
    "process_button = widgets.Button(\n",
    "    description='üöÄ Process Document',\n",
    "    button_style='success',\n",
    "    layout=widgets.Layout(width='200px', height='40px')\n",
    ")\n",
    "\n",
    "output_area = widgets.Output()\n",
    "\n",
    "def process_document(button):\n",
    "    with output_area:\n",
    "        output_area.clear_output()\n",
    "        \n",
    "        try:\n",
    "            print(\"üîÑ Initializing processor...\")\n",
    "            processor, current_config = create_processor()\n",
    "            \n",
    "            print(f\"üìÑ Processing document: {get_document_source()}\")\n",
    "            print(f\"üîß Configuration: {current_config['llm']['provider']} - {current_config['processing']['template']}\")\n",
    "            \n",
    "            # Process the document\n",
    "            result = processor.process_document(\n",
    "                source=get_document_source(),\n",
    "                template=template_type.value\n",
    "            )\n",
    "            \n",
    "            print(\"\\n‚úÖ Processing completed successfully!\")\n",
    "            print(f\"üìÅ Session path: {result['session_path']}\")\n",
    "            print(f\"üìÑ Final document: {result['final_document']}\")\n",
    "            print(f\"üìä Processed sections: {result['processed_sections']}\")\n",
    "            \n",
    "            # Store result for analysis\n",
    "            global processing_result\n",
    "            processing_result = result\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"‚ùå Error during processing: {str(e)}\")\n",
    "            import traceback\n",
    "            traceback.print_exc()\n",
    "\n",
    "process_button.on_click(process_document)\n",
    "\n",
    "display(widgets.VBox([process_button, output_area]))"
   ]
  },
