    "## 7. Quality Assessment"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Quality assessment tools\n",
    "def run_quality_assessment():\n",
    "    if 'processing_result' not in globals():\n",
    "        print(\"‚ö†Ô∏è No processing results available.\")\n",
    "        return\n",
    "    \n",
    "    print(\"üîç Quality Assessment\")\n",
    "    print(\"=\" * 50)\n",
    "    \n",
    "    analysis = processing_result['analysis']\n",
    "    \n",
    "    # Content preservation check\n",
    "    if 'content_analysis' in analysis and 'error' not in analysis['content_analysis']:\n",
    "        content = analysis['content_analysis']\n",
    "        \n",
    "        print(\"üìä Content Preservation:\")\n",
    "        change_percent = abs(content['character_count']['change_percent'])\n",
    "        \n",
    "        if change_percent < 5:\n",
    "            print(\"  ‚úÖ Excellent preservation (< 5% change)\")\n",
    "        elif change_percent < 15:\n",
    "            print(\"  ‚ö†Ô∏è Good preservation (< 15% change)\")\n",
    "        else:\n",
    "            print(\"  ‚ùå Significant changes (> 15% change)\")\n",
    "        \n",
    "        print(f\"  üìà Character change: {content['character_count']['change_percent']:.1f}%\")\n",
    "        \n",
    "        # Equation preservation\n",
    "        if content['equations_preserved']['preserved']:\n",
    "            print(\"  ‚úÖ All equations preserved\")\n",
    "        else:\n",
    "            print(f\"  ‚ö†Ô∏è Equations: {content['equations_preserved']['original']} ‚Üí {content['equations_preserved']['final']}\")\n",
    "    \n",
    "    # Processing efficiency\n",
    "    summary = analysis['processing_summary']\n",
    "    chunks_per_step = summary['total_chunks'] / max(summary['processing_steps'], 1)\n",
    "    \n",
    "    print(f\"\\n‚ö° Processing Efficiency:\")\n",
    "    print(f\"  üì¶ Chunks processed: {summary['total_chunks']}\")\n",
    "    print(f\"  üîÑ Processing steps: {summary['processing_steps']}\")\n",
    "    print(f\"  üìä Efficiency ratio: {chunks_per_step:.2f} chunks/step\")\n",
    "    \n",
    "    # Overall quality score\n",
    "    quality_score = 100\n",
    "    \n",
    "    if 'content_analysis' in analysis and 'error' not in analysis['content_analysis']:\n",
    "        content = analysis['content_analysis']\n",
    "        change_percent = abs(content['character_count']['change_percent'])\n",
    "        \n",
    "        if change_percent > 20:\n",
    "            quality_score -= 30\n",
    "        elif change_percent > 10:\n",
    "            quality_score -= 15\n",
    "        \n",
    "        if not content['equations_preserved']['preserved']:\n",
    "            quality_score -= 20\n",
    "    \n",
    "    print(f\"\\nüèÜ Overall Quality Score: {quality_score}/100\")\n",
    "    \n",
    "    if quality_score >= 90:\n",
    "        print(\"  ‚úÖ Excellent quality\")\n",
    "    elif quality_score >= 75:\n",
    "        print(\"  ‚ö†Ô∏è Good quality\")\n",
    "    else:\n",
    "        print(\"  ‚ùå Needs improvement\")\n",
    "\n",
    "# Quality assessment button\n",
    "quality_button = widgets.Button(\n",
    "    description='üîç Quality Check',\n",
    "    button_style='success',\n",
    "    layout=widgets.Layout(width='150px')\n",
    ")\n",
    "\n",
    "quality_output = widgets.Output()\n",
    "\n",
    "def show_quality(button):\n",
    "    with quality_output:\n",
    "        quality_output.clear_output()\n",
    "        run_quality_assessment()\n",
    "\n",
    "quality_button.on_click(show_quality)\n",
    "\n",
    "display(widgets.VBox([quality_button, quality_output]))"
   ]
