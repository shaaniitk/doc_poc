    "    \n",
    "    # Processing summary\n",
    "    summary = analysis['processing_summary']\n",
    "    print(f\"Session ID: {summary['session_id']}\")\n",
    "    print(f\"Total Chunks: {summary['total_chunks']}\")\n",
    "    print(f\"Processing Steps: {summary['processing_steps']}\")\n",
    "    \n",
    "    # Content analysis\n",
    "    if 'content_analysis' in analysis and 'error' not in analysis['content_analysis']:\n",
