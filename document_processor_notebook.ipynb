    "## 3. Document Input Selection"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Document input options\n",
    "print(\"üìÑ Document Input Selection\")\n",
    "\n",
    "input_type = widgets.RadioButtons(\n",
    "    options=['Local File', 'URL', 'arXiv ID'],\n",
    "    value='Local File',\n",
    "    description='Input Type:'\n",
    ")\n",
    "\n",
    "file_path = widgets.Text(\n",
    "    value='bitcoin_whitepaper.tex',\n",
    "    description='File Path:',\n",
    "    style={'description_width': 'initial'}\n",
    ")\n",
    "\n",
    "url_input = widgets.Text(\n",
    "    value='https://example.com/document.tex',\n",
    "    description='URL:',\n",
    "    style={'description_width': 'initial'}\n",
    ")\n",
    "\n",
    "arxiv_id = widgets.Text(\n",
    "    value='2301.12345',\n",
    "    description='arXiv ID:',\n",
    "    style={'description_width': 'initial'}\n",
    ")\n",
    "\n",
    "# Function to show/hide input fields based on selection\n",
    "def on_input_type_change(change):\n",
    "    if change['new'] == 'Local File':\n",
    "        file_path.layout.display = 'block'\n",
    "        url_input.layout.display = 'none'\n",
    "        arxiv_id.layout.display = 'none'\n",
    "    elif change['new'] == 'URL':\n",
    "        file_path.layout.display = 'none'\n",
    "        url_input.layout.display = 'block'\n",
    "        arxiv_id.layout.display = 'none'\n",
    "    else:  # arXiv ID\n",
    "        file_path.layout.display = 'none'\n",
    "        url_input.layout.display = 'none'\n",
    "        arxiv_id.layout.display = 'block'\n",
    "\n",
    "input_type.observe(on_input_type_change, names='value')\n",
    "\n",
    "# Initial setup\n",
    "url_input.layout.display = 'none'\n",
    "arxiv_id.layout.display = 'none'\n",
    "\n",
    "display(widgets.VBox([input_type, file_path, url_input, arxiv_id]))"
   ]
